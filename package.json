{
  "name": "keyboardist",
  "version": "1.0.4",
  "description": "simple global keyboard manager",
  "main": "lib/keyboardist.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --trailing-comma es5 --single-quote --write",
    "format": "yarn run prettier './src/*.js*'",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/keyboardist.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/keyboardist.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/keyboardist.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/keyboardist.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "keywords": [
    "keyboard",
    "shortcuts"
  ],
  "author": "Armando Sosa <arm.sosa@gmail.com>",
  "homepage": "https://github.com/soska/keyboardist.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:soska/keyboardist.js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "prettier": "^1.8.2",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "cross-env": "^5.1.5",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
